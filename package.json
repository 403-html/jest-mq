{
  "name": "jest-mq",
  "version": "1.0.1",
  "description": "A Jest framework for testing message queues like RabbitMQ and Kafka.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --config jest.config.js --coverage",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "format:write": "prettier --write \"src/**/*.ts\"",
    "quality": "npm run format:check && npm run typecheck && npm test",
    "prepublishOnly": "npm run build && npm test",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./matchers": {
      "types": "./dist/matchers/index.d.ts",
      "default": "./dist/matchers/index.js"
    }
  },
  "keywords": [
    "jest",
    "jest-matchers",
    "mq",
    "queue",
    "message-queue",
    "rabbitmq",
    "kafka",
    "test",
    "testing",
    "mock",
    "stub"
  ],
  "bugs": {
    "url": "https://github.com/403-html/jest-mq/issues"
  },
  "homepage": "https://github.com/403-html/jest-mq#readme",
  "engines": {
    "node": ">=24.12.0"
  },
  "license": "MIT",
  "dependencies": {
    "jest-matcher-utils": "^30.2.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.7",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "typescript": "^5.6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/403-html/jest-mq.git"
  },
  "author": "403-html"
}
